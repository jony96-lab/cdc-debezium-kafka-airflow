{
  "name": "mysql-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "host.docker.internal",
    "database.port": "3306",
    "database.user": "######", 
    "database.password": "######",
    "topic.prefix": "mysql-server",
    "database.include.list": "pos_bi_db",
    "table.include.list": "pos_bi_db.categorias,pos_bi_db.entidades,pos_bi_db.productos,pos_bi_db.ventas,pos_bi_db.detalle_ventas,pos_bi_db.compras,pos_bi_db.detalle_compras,pos_bi_db.cierres_caja,pos_bi_db.pagos_clientes",
    "include.schema.changes": "true",
    "database.connectionTimeZone": "UTC",
    "database.server.id": "100",
    "time.precision.mode": "connect",
    "decimal.handling.mode": "double",
    "date.format": "yyyy-MM-dd",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:29092",
    "schema.history.internal.kafka.topic": "dbhistory.pos",
    "schema.history.internal.kafka.replication.factor": "1",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "true",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "true"
  }
}
